	border-radius: 50%;
}

.home_wrap_bottom_top {
	position: relative;
}

.home_wrap_bottom_fixed {
	width: 50rpx;
	height: 18rpx;
	position: absolute;
	bottom: 0;
	left: 38rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 12rpx;
	background: rgb(255, 33, 0);
	color: #fff;
	border-radius: 30rpx;
}

.home_wrap_bottom_left {
	display: flex;
	justify-content: center;
	font-size: 30rpx;
	margin: 25rpx 0 0 0;
