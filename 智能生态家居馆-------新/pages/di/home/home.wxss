/* pages/di/home/home.wxss */
.home_wrap {
	width: 100vw;
	height: 500rpx;
	position: relative;
}

.home_w_im {
	width: 100%;
	height: 60%;
	position: absolute;
	top: 0;
	left: 0;
}

.swip_item image {
	width: 629rpx;
	height: 360rpx;
	border-radius: 50rpx;
}

.home_wrap_two {
	width: 100vw;
	height: 800rpx;
	display: flex;
	position: relative;
	background: red;
}

.home_two_img {
	width: 100%;
	height: 340rpx;
	position: absolute;
	top: 0;
	left: 0;
}

.home_two_text {
	font-size: 40rpx;
	position: absolute;
	top: 20rpx;
	right: 234rpx;
	color: #fff;
}

.home_wrap_three {
	width: 650rpx;
	height: 303rpx;
	position: absolute;
	top: 120rpx;
	left: 48rpx;
	background: #eee;
	border-radius: 30rpx;
	box-shadow: 2rpx 10rpx 10rpx rgb(221, 221, 221);
}

.home_wrap_three_deta {
	display: flex;
	margin: 50rpx 0 0 30rpx;
	justify-content: space-between;
}

.home_wrap_three_deta image {
	width: 270rpx;
	height: 200rpx;
}

.home_wrap_three_right {
	display: flex;
	width: 40vw;
	flex-direction: column;
}

.home_right_text {
	font-size: 26rpx;
	color: rgb(109, 109, 109);
}

.home_right_t_two {
	display: flex;
	margin: 20rpx 0 0 0;
}

.home_right_t_two text:nth-child(1) {
	color: rgb(255, 31, 0);
	font-size: 40rpx;
}

.home_right_t_two text:nth-child(2) {
	color: rgb(184, 184, 184);
	font-size: 30rpx;
	text-decoration: line-through;
	margin: 10rpx 0 0 10rpx;
}

.home_right_t_three {
	width: 160rpx;
