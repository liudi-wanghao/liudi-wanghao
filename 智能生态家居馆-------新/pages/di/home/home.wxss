	color: rgb(56, 77, 76);
}

.three {
	color: rgb(196, 35, 42);
}

.order {
	display: flex;
	justify-content: center;
	align-items: center;
	margin: 20rpx 0 0 0;
}

.order .line {
	display: inline-block;
	width: 100px;
	border-top: 1px solid #ccc;
}

.order .txt {
	color: #686868;
	vertical-align: middle;
}

