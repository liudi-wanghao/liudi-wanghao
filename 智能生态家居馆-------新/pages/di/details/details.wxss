	flex-direction: column;
	background: #f8f8f8;
}

.detail_wrap {
	width: 100vw;
	height: 400rpx;
	display: flex;
	position: relative;
}

.detail_wrap image {
	width: 100vw;
	height: 400rpx;
}

.detail_wrap icon {
	position: absolute;
	width: 70rpx;
	height: 70rpx;
	border-radius: 50%;
	background: rgb(97, 97, 97);
	color: #fff;
	display: flex;
	justify-content: center;
	align-items: center;
	/* opacity: .5; */
}

.detail_wrap icon:nth-child(2) {
	position: absolute;
	top: 30rpx;
	right: 30rpx;
}

.detail_wrap icon:nth-child(3) {
	position: absolute;
	top: 30rpx;
	right: 130rpx;
}

